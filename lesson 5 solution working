import java.util.Scanner;

public class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int count;
    int age;
    System.out.print("input year of birth");
    count = sc.nextInt();
    System.out.print("input age");
    age = sc.nextInt();
    System.out.print(((count + 5)*50 + age - 250 )/100);
  }
}
